{
  "metadata": [
    {
      "src": [
        {
          "src": "../UraniumUI/src",
          "files": [
            "UraniumUI/bin/Debug/net8.0/UraniumUI.dll",
            "UraniumUI.Material/bin/Debug/net8.0/UraniumUI.Material.dll",
            "UraniumUI.Blurs/bin/Debug/net8.0/UraniumUI.Blurs.dll",
            "UraniumUI.Dialogs.CommunityToolkit/bin/Debug/net8.0/UraniumUI.Dialogs.CommunityToolkit.dll",
            "UraniumUI.Dialogs.Mopups/bin/Debug/net8.0/UraniumUI.Dialogs.Mopups.dll",
            "UraniumUI.Icons.FontAwesome/bin/Debug/net8.0/UraniumUI.Icons.FontAwesome.dll",
            "UraniumUI.Icons.MaterialIcons/bin/Debug/net8.0/UraniumUI.Icons.MaterialIcons.dll",
            "UraniumUI.Icons.MaterialSymbols/bin/Debug/net8.0/UraniumUI.Icons.MaterialSymbols.dll",
            "UraniumUI.Icons.SegoeFluent/bin/Debug/net8.0/UraniumUI.Icons.SegoeFluent.dll",
            "UraniumUI.Validations.DataAnnotations/bin/Debug/net8.0/UraniumUI.Validations.DataAnnotations.dll"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "UraniumUI",
      "_appTitle": "UraniumUI",
      "_enableSearch": true,
      "pdf": false
    }
  }
}